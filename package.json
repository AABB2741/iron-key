{
  "name": "iron-key",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:web": "turbo run build:web",
    "build:desktop": "turbo run build:desktop",
    "lint": "turbo run lint",
    "docker:dev": "docker compose -f docker-compose.dev.yml up --wait",
    "docker:app": "docker compose -f docker-compose.app.yml up --wait",
    "docker:down": "docker compose -f docker-compose.dev.yml -f docker-compose.app.yml down"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=22"
  }
}