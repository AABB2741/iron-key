{
  "recommendations": [
    "tauri-apps.tauri-vscode",
    "rust-lang.rust-analyzer"
  ],
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "\\.(tf|tfvars)$",
        "cmd": "terraform fmt infra --recursive"
      }
    ]
  }
}